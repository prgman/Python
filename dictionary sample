fruit = ["사과","사과", "바나나", "바나나", "딸기", "키위", "복숭아","복숭아","복숭아"]

d={}

for f in fruit:
  if f in d :         #"사과"라는 key가 d라는 딕셔너리에 들어있어?
    d[f] = d[f] + 1   #그럼 "사과"갯수를 하나 올려줘
  else:
    d[f] = 1          #만약 "사과"라는 key가 없으면, 그걸 딕셔너리에 넣고 value는 1로 만들어줘

print(d)              #{'사과': 2, '바나나': 2, '딸기': 1, '키위': 1, '복숭아': 3}
