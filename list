x = list()
y = []
print(x)    #[]
print(y)    #[]

x= [4,2,3,1]
y=["sang", "hello",2]
x[2] = 10
print(x+y)   #4,2,3,1,"sang","hello",2
print(y[0])   #sang
num_element = len(x)    #4
print(num_element)   

k=sorted(x)     #1,2,4,10
print(k)

z= sum(x)     #17
print(z)

for n in x:    # 4 enter 2 enter 10 enter 1
  print(n)

print(x.index(1))  #3
print(y.index("sang"))  #0
print('hello' in y)   #True
if "hello" in y:      
  print("hello 있삼")    #hello 있삼
