x = dict()
y = {}
print(x)
print(y)

x = {
  "name" : "sangpil",
  "age": 45,
  0:"hello"
}

print(x["name"])
print(x["age"])
print(x[0])
print("age"in x)
print(x.keys())
print(x.values())
for key in x:
  print("key:" + str(key))
  print("value:" + str(x[key]))

x[0] = "world"
x[3] = "hello"
print(x)
