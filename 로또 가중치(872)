import random
from collections import Counter

numbers = list(range(1,46))
weight_list = [1]*9 + [2]*9 + [3]*9 + [4]*9 + [5]*9
weight_lotto_872 = [148/872*100+ 2/30*100]+[140/872*100+ 6/30*100]+[135/872*100+ 4/30*100]+[143/872*100+4/30*100]+[134/872*100+ 3/30*100]+ [135/872*100+ 4/30*100]+[136/872*100+ 3/30*100]+[138/872*100+ 5/30*100]+[110/872*100+ 3/30*100]+[143/872*100+ 4/30*100]+ [140/872*100+ 2/30*100]+ [146/872*100+ 5/30*100]+[148/872*100+ 6/30*100]+[139/872*100+ 2/30*100]+[134/872*100+ 4/30*100]+[135/872*100+ 6/30*100]+ [148/872*100+ 7/30*100]+[136/872*100+ 4/30*100]+[136/872*100+ 4/30*100]+[144/872*100+ 3/30*100]+ [135/872*100+ 4/30*100]+[108/872*100+ 7/30*100]+[113/872*100+ 2/30*100]+[138/872*100+ 5/30*100]+[132/872*100+6/30*100]+[137/872*100+4/30*100]+ [152/872*100+3/30*100]+[121/872*100+6/30*100]+[118/872*100+4/30*100]+[131/872*100+7/30*100]+ [138/872*100+3/30*100]+[121/872*100+8/30*100]+[147/872*100+6/30*100]+[152/872*100+7/30*100]+[129/872*100+3/30*100]+[131/872*100+3/30*100]+ [136/872*100+2/30*100]+[136/872*100+6/30*100]+[140/872*100+8/30*100]+[144/872*100+6/30*100]+ [120/872*100+5/30*100]+[130/872*100+5/30*100]+[161/872*100+10/30*100]+[131/872*100+5/30*100]+[135/872*100+4/30*100]




print(weight_lotto_872)

lotto = []

for i in range(10):
  numbers_weighted = random.choices(numbers, weights=weight_lotto_872, k=872*6)

#print(sorted(numbers_weighted))

  print ("---Counter()---")
